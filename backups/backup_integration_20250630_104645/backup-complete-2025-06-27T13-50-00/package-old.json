{
  "name": "matchit-analytics",
  "version": "3.0.0",
  "description": "MatchIt Analytics System - Advanced Business Intelligence",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:analytics": "jest tests/analytics/",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "analytics:setup": "node scripts/setup-analytics.js",
    "analytics:migrate": "node scripts/migrate-analytics.js",
    "analytics:test": "node scripts/test-analytics.js",
    "analytics:generate-report": "node scripts/generate-report.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "expo build:web",
    "build:backend": "echo 'Backend build complete'",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "docker:build": "docker build -t matchit-analytics .",
    "docker:run": "docker run -p 3000:3000 matchit-analytics"
  },
  "keywords": [
    "analytics",
    "business-intelligence", 
    "dating-app",
    "metrics",
    "dashboard",
    "reporting"
  ],
  "author": "MatchIt Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "body-parser": "^1.20.2",
    "express-rate-limit": "^6.7.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.1.4",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "moment-timezone": "^0.5.43",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "csvtojson": "^2.0.10",
    "json2csv": "^6.1.0",
    "pdfkit": "^0.13.0",
    "xlsx": "^0.18.5",
    "recharts": "^2.6.2",
    "chart.js": "^4.3.0",
    "d3": "^7.8.5",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-linear-gradient": "^2.8.1",
    "expo": "^49.0.0",
    "expo-linear-gradient": "^12.3.0",
    "@expo/vector-icons": "^13.0.0",
    "lucide-react": "^0.263.1",
    "lucide-react-native": "^0.263.1",
    "async-storage": "@react-native-async-storage/async-storage",
    "node-fetch": "^3.3.1",
    "axios": "^1.4.0",
    "ws": "^8.13.0",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "redis": "^4.6.7",
    "bull": "^4.10.4",
    "agenda": "^5.0.0",
    "winston": "^3.9.0",
    "morgan": "^1.10.0",
    "prometheus-client": "^14.2.0",
    "statsd-client": "^0.4.7",
    "newrelic": "^10.3.0",
    "sentry": "^7.57.0",
    "datadog-metrics": "^0.9.3"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/express": "^4.17.17",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node-cron": "^3.0.8",
    "@types/nodemailer": "^6.4.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/ws": "^8.5.5",
    "@types/jest": "^29.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "typescript": "^5.1.3",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-native": "^4.0.0",
    "prettier": "^2.8.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.0",
    "wait-on": "^7.0.1",
    "docker": "^0.2.14"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.60.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matchit/analytics.git"
  },
  "bugs": {
    "url": "https://github.com/matchit/analytics/issues"
  },
  "homepage": "https://github.com/matchit/analytics#readme",
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "server/**/*.js",
      "!server/**/*.test.js",
      "!server/migrations/**",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-native/all"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-native"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "react/prop-types": "off",
      "react-native/no-inline-styles": "warn"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest": true,
      "react-native/react-native": true
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "analytics": {
    "version": "3.0.0",
    "features": [
      "real-time-tracking",
      "business-intelligence",
      "anomaly-detection", 
      "automated-reporting",
      "predictive-analytics",
      "dashboard-visualization",
      "alert-system",
      "data-export"
    ],
    "databases": {
      "primary": "postgresql",
      "cache": "redis",
      "search": "elasticsearch"
    },
    "apis": {
      "rest": "/api/analytics",
      "websocket": "/ws/analytics",
      "graphql": "/graphql/analytics"
    }
  }
}